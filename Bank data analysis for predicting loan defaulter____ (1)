    "from sklearn import preprocessing\n",
    "  \n",
    "# label_encoder object knows \n",
    "# how to understand word labels.\n",
    "label_encoder = preprocessing.LabelEncoder()\n",
    "  \n",
    "# Encode labels in column 'species'.\n",
    "df_new['checking_balance']= label_encoder.fit_transform(df_new['checking_balance'])\n",
    "df_new['credit_history']= label_encoder.fit_transform(df_new['credit_history'])\n",
    "df_new['purpose']= label_encoder.fit_transform(df_new['purpose'])\n",
    "df_new['savings_balance']= label_encoder.fit_transform(df_new['savings_balance'])\n",
    "df_new['employment_duration']= label_encoder.fit_transform(df_new['employment_duration'])\n",
